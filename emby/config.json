{
  "name": "emby",
  "url": "https://github.com/haberda/hassio_addons/tree/master/emby",
  "version": "4.6.1",
  "slug": "emby",
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "description": "Emby Media Server",
  "startup": "system",
  "boot": "manual",
  "map": [
    "share:rw",
    "media:rw"
  ],
  "ports": {
    "8096/tcp": 8096,
    "8920/tcp": 8920,
    "33791/tcp": 33791,
    "33796/tcp": 33796,
    "7359/udp": 7359,
    "1900/udp": 1900
  },
  "ports_description": {
    "8096/tcp": "http webui",
    "8920/tcp": "https webui",
    "33791/tcp": "external http webui",
    "33796/tcp": "external https webui",
    "7359/udp": "7359",
    "1900/udp": "1900"
  },
  "webui": "http://[HOST]:[PORT:8096]",
  "snapshot_exclude":["*/cache/", "*/logs/", "*/transcoding-temp/", "*/metadata/"],
  "image": "ghcr.io/haberda/emby/{arch}"
}
