{
  "name": "Nextcloud",
  "url": "https://github.com/haberda/hassio_addons/tree/master/nextcloud",
  "version": "21.0.2",
  "slug": "nextcloud",
  "ingress": false,
  "ingress_port": 0,
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "description": "Nextcloud for Home Assistant",
  "startup": "system",
  "boot": "manual",
  "map": [
    "share:rw",
    "media:rw",
    "ssl:rw"
  ],
  "ports": {
    "443/tcp": 8099,
    "80/tcp": null
  },
  "ports_description": {
    "443/tcp": "https port",
    "80/tcp": "http port"
  },
  "webui": "https://[HOST]:[PORT:443]",
  "panel_icon": "mdi:cloud",
  "snapshot_exclude":["*/nextcloud/log/"],
  "image": "ghcr.io/haberda/nextcloud/{arch}"
}
